{
  "name": "nude-solutions-rails-react-assignment",
  "private": true,
  "dependencies": {
    "@babel/preset-react": "^7.10.4",
    "@rails/webpacker": "5.1.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react_ujs": "^2.6.1"
  },
  "scripts": {
    "prettier": "prettier -l app/javascript/**/*/*.{js,jsx}",
    "prettier:format": "yarn run prettier --write",
    "eslint": "eslint --ext .js --ext .jsx app/javascript --quiet --cache",
    "eslint:format": "yarn run eslint --fix"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "jest --findRelatedTests"
    ],
    "app/javascript/**/*/*.{js,jsx}": [
      "prettier -l --write",
      "git add",
      "yarn run eslint:format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/enzyme": "^3.10.5",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "expect": "^26.4.0",
    "husky": ">=4",
    "jest": "^26.4.0",
    "lint-staged": ">=10",
    "prettier": "^2.0.5",
    "webpack-dev-server": "^3.11.0"
  }
}
